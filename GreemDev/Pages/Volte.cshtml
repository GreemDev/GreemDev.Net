@page
@model GreemDev.Pages.Volte

@{
    Layout = "_Layout";
    ViewData["Title"] = "Volte";
}

<!DOCTYPE html>

<!DOCTYPE HTML>
<html>
<head>
    <title>Volte</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="volte/assets/css/main.css" />
    <noscript><link rel="stylesheet" href="volte/assets/css/noscript.css" /></noscript>
    <link rel="icon" href="volte/images/logo.png" type="image/x-icon">
</head>
<body class="is-preload">

<div id="wrapper">

    <header id="header">
        <div class="logo">
            <img src="volte/images/logo.png" width="85"></img>
        </div>
        <div class="content">
            <div class="inner">
                <h1>Volte</h1>
                <p>A free, robust, open-source Discord bot written in C#.</p>
            </div>
        </div>
        <nav>
            <ul>
                <li><a href="https://github.com/GreemDev/Volte" target="_blank">GitHub</a></li>
                <li><a href="https://github.com/GreemDev/Volte/wiki" target="_blank">Docs</a></li>
                <li><a href="https://discord.greemdev.net" target="_blank">Discord</a></li>
                <li><a href="https://discordapp.com/oauth2/authorize?client_id=320942091049893888&scope=bot&permissions=8" target="_blank">Invite</a></li>
            </ul>
        </nav>
    </header>

    <footer id="footer">
        <p class="copyright">&copy; GreemDev.</p>
    </footer>

</div>

<div id="bg"></div>

<script src="volte/assets/js/jquery.min.js"></script>
<script src="volte/assets/js/browser.min.js"></script>
<script src="volte/assets/js/breakpoints.min.js"></script>
<script src="volte/assets/js/util.js"></script>
<script src="volte/assets/js/main.js"></script>

</body>
</html>
